{
	"name": "obsidian-oneseater",
	"version": "1.0.0",
	"description": "A simple plugin to fiddle around with motorsport data.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs --watch",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest --ui",
		"test-run": "vitest run",
		"coverage": "vitest run --coverage",
		"docs": "typedoc --entryPointStrategy expand ./src",
		"publish": "npm run coverage && npm run docs && npm run build"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^25.0.3",
		"@typescript-eslint/eslint-plugin": "8.50.1",
		"@typescript-eslint/parser": "8.50.1",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"builtin-modules": "5.0.0",
		"esbuild": "0.27.2",
		"obsidian": "latest",
		"tslib": "2.8.1",
		"typedoc": "^0.28.15",
		"typescript": "5.9.3",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"sim-ecs": "^0.6.4",
		"xstate": "^5.25.0"
	}
}
